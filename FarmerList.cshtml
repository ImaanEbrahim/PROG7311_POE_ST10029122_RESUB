@model IEnumerable<PROG3A_POE.Models.ApplicationUser>

<h2>Farmer List</h2>

<a asp-action="AddFarmerProfile" class="btn btn-primary mb-3">Add New Farmer</a>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var farmer in Model)
        {
            <tr>
                <td>@farmer.UserName</td>
                <td>@farmer.Email</td>
            </tr>
        }
    </tbody>
</table>
